{
  "expo": {
    "name": "TradingApp",
    "slug": "tradingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SkyshowAPPicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash--new.png",
      "resizeMode": "contain",
      "backgroundColor": "#020817"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you use them on your profile."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SkyshowAPPicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "skyshow.ng",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "owner": "miraclemarcel",
    "extra": {
      "eas": {
        "projectId": "b4bcb5ab-8a3a-47eb-a9fc-c06f7e30bf19"
      }
    }
  }
}
